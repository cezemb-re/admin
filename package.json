    "sass-loader": "^12.1.0",
    "size-limit": "^5.0.1",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint"
    }
  }
