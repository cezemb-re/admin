  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@cezembre/forms": "^1.5.3",
    "@cezembre/ui": "^0.1.6",
    "@release-it/conventional-changelog": "^2.0.1",
