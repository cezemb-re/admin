    "build": "npm run build-scripts && npm run build-styles",
    "watch": "nodemon --watch src -e ts,tsx,scss --exec 'npm run build'",
    "test": "echo 'No tests written",
    "lint": "eslint",
    "prepare": "npm run lint && npm run build",
    "release": "release-it"
  },
