    "react-router-dom": ">=5.2.0",
    "draft-js": ">=0.11.7"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@cezembre/forms": "^1.5.6",
    "@cezembre/fronts": "^0.1.11",
    "@cezembre/ui": "^0.2.12",
    "@release-it/conventional-changelog": "^3.0.1",
    "@types/draft-js": "^0.11.3",
    "@types/lodash.isequal": "^4.5.5",
