    "react",
    "admin",
    "cezembre"
  ],
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build:scripts": "tsc --build",
    "build:styles": "sass --load-path=node_modules/ ./src/style.scss ./lib/style.css && rsync -av --include='*/' --include='**/*.scss' --exclude='*' ./src/ ./lib/",
    "build": "npm run build:scripts && npm run build:styles",
    "watch": "nodemon --watch src -e ts,tsx,scss --exec 'npm run build'",
    "test": "echo 'No tests written",
    "lint": "eslint",
    "prepare": "npm run lint && npm run build",
    "release": "npm run build && release-it"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "@cezembre/fronts": ">=0.1.11",
    "@cezembre/forms": ">=1.5.6",
